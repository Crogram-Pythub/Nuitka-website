{
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.exclude": {
        "**/*.build": true,
        "**/*.pyc": true,
        "**/__pycache__": true,
        "**/*.orig": true,
        "**/*.tmp": true,
    },
    "restructuredtext.confPath": "",
    "python.formatting.provider": "black",
    "python.analysis.extraPaths": ["Nuitka-develop"],
    "cSpell.words": [
        "Annuitka",
        "appdirs",
        "asyncgen",
        "backport",
        "backported",
        "bdist",
        "bytecode",
        "ccache",
        "CEST",
        "chdir",
        "clcache",
        "conda",
        "ctypes",
        "devcontainer",
        "devel",
        "distutils",
        "Doxygen",
        "elif",
        "EPEL",
        "Europython",
        "Gitter",
        "gsoc",
        "Hayen",
        "imageio",
        "imopen",
        "importlib",
        "Impressum",
        "isort",
        "ispkg",
        "joblib",
        "jpegoptim",
        "kayhayen",
        "libc",
        "libpython",
        "listdir",
        "loky",
        "lxml",
        "metaclass",
        "MSVC",
        "Multidist",
        "noarch",
        "nofollow",
        "Nuitka",
        "numberize",
        "onefile",
        "optipng",
        "Oreans",
        "pipenv",
        "Podman",
        "pyenv",
        "pyproject",
        "pyside",
        "pystone",
        "PYTHONHASHSEED",
        "PYTHONPATH",
        "redist",
        "Roadmap",
        "runpy",
        "Scons",
        "Setuptools",
        "SIGSEGV",
        "Tkinter",
        "traceback",
        "tracebacks",
        "ucrt",
        "uncompiled",
        "unstripped",
        "virtualenv",
        "whitebox",
        "zstandard"
    ],
    "esbonio.server.enabled": true,
    "esbonio.sphinx.confDir": "",
    "editor.wordWrap": "bounded",
    "githubPullRequests.ignoredPullRequestBranches": [
        "main"
    ],
    "cSpell.customDictionaries": {
        "custom-dictionary-workspace": {
            "name": "custom-dictionary-workspace",
            "path": "${workspaceFolder:Nuitka-website}/.cspell/custom-dictionary-workspace.txt",
            "addWords": true,
            "scope": "workspace"
        }
    },
    "editor.defaultFormatter": "vscode.css-language-features",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "grammarly.selectors": [
        {
            "language": "restructuredtext",
            "scheme": "file"
        }
    ],
}
